if not game:IsLoaded() then
    repeat wait()
    until game:IsLoaded()
end

loadstring(game:HttpGet('https://raw.githubusercontent.com/RealTheVictor/EZ/main/E.Z.%20DHA/uilib'))()

local reps = game:GetService("ReplicatedStorage")
local idk = reps.Catalog.Body.LeftLegs.UnU

-- [Main Tab] -----------------------------------------------------------------------------------------------------------------------------------------------------
local MainTab = library:AddTab("Main");
local MainColumn1 = MainTab:AddColumn();
local Avatar = MainColumn1:AddSection("Avatar")
local Extra = MainColumn1:AddSection("Extra")

Avatar:AddButton({text = "Headless", callback = function()
    idk:FireServer("wear", 134082579)
end});
Avatar:AddButton({text = "Korblox", callback = function()
    idk:FireServer("wear", 139607718)
end});
Avatar:AddButton({text = "Headless & Korblox", callback = function()
    idk:FireServer("wear", 134082579)
    idk:FireServer("wear", 139607718)
end});
Avatar:AddButton({text = "Trick's Purple Hat", callback = function()
    idk:FireServer("wear", 47697285)
end});
Avatar:AddDivider("(Take off accessories)")
Avatar:AddButton({text = "Become Trick", callback = function()
    idk:FireServer("wear", 47697285)
    idk:FireServer("wear", 7262883976)
    idk:FireServer("wear", 11026529354)
    idk:FireServer("wear", 2510235063)
    idk:FireServer("wear", 619537468)
    idk:FireServer("wear", 619536621)
    idk:FireServer("wear", 658832070)
    idk:FireServer("wear", 754636589)
    idk:FireServer("wear", 5319931619)
    idk:FireServer("skintone", "Really black")
end});

local aimkey = "e"
Extra:AddButton({text = "FPS Gui", callback = function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/RealTheVictor/EZ/main/E.Z.%20DHA/idk'))()
end});

Extra:AddDivider("FPS Gui Keybinds")

Extra:AddButton({text = "Set FPS Gui key to E", callback = function()
    aimkey = "e"
end});

Extra:AddButton({text = "Set FPS Gui key to V", callback = function()
    aimkey = "e"
end});

local MainColumn2 = MainTab:AddColumn()
local Scaling = MainColumn2:AddSection("Scaling")

Scaling:AddSlider({text = "Body Height Scale", min = 0, max = 1000, value = 100, callback = function(size)
    idk:FireServer("scale","BodyHeightScale",size)
end});

Scaling:AddSlider({text = "Body Width Scale", min = 0, max = 1000, value = 50, callback = function(size)
    idk:FireServer("scale","BodyWidthScale",size)
end});

Scaling:AddSlider({text = "Head Scale", min = 0, max = 1000, value = 100, callback = function(size)
    idk:FireServer("scale","HeadScale",size)
end});

