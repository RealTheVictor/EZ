-- Scripts:



local function UVVYZZ_fake_script() -- FriendsButton.LocalScript 


	local script = Instance.new('LocalScript', FriendsButton)



	local on = false


	


	script.Parent.MouseButton1Click:Connect(function()


		if on == false then


			on = true


			script.Parent.Parent.Parent.Menus.Friends.Visible = true


			script.Parent.Parent.Parent.Menus.Home.Visible = false


			script.Parent.Parent.Parent.Menus.Settings.Visible = false


		else


			if on == true then


				on = false


				script.Parent.Parent.Parent.Menus.Friends.Visible = false


			end


		end


	end)


	


	script.Parent.MouseEnter:Connect(function()


		script.Parent:TweenSize(


			UDim2.new(0, 66,0, 71),


			Enum.EasingDirection.Out,


			Enum.EasingStyle.Sine,


			0.1,


			true


		)


		wait(0.1)


		script.Parent.Parent.Parent.ButtonHover.Visible = true


		script.Parent.Parent.Parent.ButtonHover:FindFirstChild("TextLabel").Text = "Friends"


	end)


	


	script.Parent.MouseLeave:Connect(function()


		script.Parent:TweenSize(


			UDim2.new(0.185, 0,1, 0),


			Enum.EasingDirection.In,


			Enum.EasingStyle.Sine,


			0.1,


			true


		)


		script.Parent.Parent.Parent.ButtonHover.Visible = false


	end)


end


coroutine.wrap(UVVYZZ_fake_script)()


