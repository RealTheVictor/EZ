local function FUEWV_fake_script() -- Button_4.LocalScript 


	local script = Instance.new('LocalScript', Button_4)



	script.Parent.MouseButton1Click:Connect(function()


		function l(a, _)


			local b = Instance.new("Weld")


			b.Part0 = a.Parent


			b.Part1 = _.Parent


			b.C0 = a.CFrame


			b.C1 = _.CFrame


			b.Parent = a.Parent


			return b


		end


		local function b(c, d, b, _, a, e)


			local f = Instance.new("Weld")


			f.Name = c


			f.Part0 = b


			f.Part1 = _


			f.C0 = a


			f.C1 = e


			f.Parent = d


			return f


		end


		local function a(_, b)


			for _, _ in pairs(_:GetChildren()) do


				if _:IsA("Attachment") and _.Name == b then


					return _


				elseif not _:IsA("Accoutrement") and not _:IsA("Tool") then


					local _ = a(_, b)


					if _ then


						return _


					end


				end


			end


		end


		function k(_, e)


			e.Parent = _


			local d = e:FindFirstChild("Handle")


			if d then


				local c = d:FindFirstChildOfClass("Attachment")


				if c then


					local _ = a(_, c.Name)


					if _ then


						l(_, c)


					end


				else


					local c = chr.Character:FindFirstChild("Head")


					if c then


						local _ = CFrame.new(0, 0.5, 0)


						local a = e.AttachmentPoint


						b("HeadWeld", c, c, d, _, a)


					end


				end


			end


		end


		local _ = 124730194


		local _ = game:GetObjects("rbxassetid://" .. tostring(_))[1]


		k(game.Players.LocalPlayer.Character, _)


	end)


end


coroutine.wrap(FUEWV_fake_script)()


local function IXSF_fake_script() -- Friends.LocalScript 


	local script = Instance.new('LocalScript', Friends)



	local tps = game:GetService("TeleportService")


	local lp = game:GetService("Players").LocalPlayer


	local friends = lp:GetFriendsOnline()


	local template = script.Parent.UIObjects.Template


	


	for i, v in pairs(friends) do


		local newframe = template:Clone()


		if v.LastLocation == "Website" then


			newframe:Destroy()


		else


			newframe.Username.Text = v.UserName


			newframe.DisplayName.Text = v.DisplayName


			newframe.ImageLabel.Image = "https://roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username="..v.UserName


			newframe.PlaceId.Text = v.GameId


			newframe.Parent = script.Parent.ScrollingFrame


			if v.LastLocation then


				newframe.Game.Text = v.LastLocation


				newframe.GameId.Text = v.GameId


				newframe.JoinButton.MouseButton1Click:Connect(function()


					tps:TeleportToPlaceInstance(v.PlaceId, v.GameId, lp)


				end)


			end


		end


	end


end


coroutine.wrap(IXSF_fake_script)()


local function LIJQGCK_fake_script() -- TextButton.LocalScript 


	local script = Instance.new('LocalScript', TextButton)



	local tps = game:GetService("TeleportService")


	local lp = game:GetService("Players").LocalPlayer


	local friends = lp:GetFriendsOnline()


	local template = script.Parent.Parent.UIObjects.Template


	


	script.Parent.MouseButton1Click:Connect(function()


		for i, v in pairs(script.Parent.Parent.ScrollingFrame:GetChildren()) do


			if v.Name == "Template" then


				v:Destroy()


			end


		end


		for i, p in pairs(friends) do


			local newframe = template:Clone()


			if p.LastLocation == "Website" then


				newframe:Destroy()


			else


				newframe.Username.Text = p.UserName


				newframe.DisplayName.Text = p.DisplayName


				newframe.ImageLabel.Image = "https://roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username="..p.UserName


				newframe.PlaceId.Text = p.GameId


				newframe.Parent = script.Parent.Parent.ScrollingFrame


				if p.LastLocation then


					newframe.Game.Text = p.LastLocation


					newframe.GameId.Text = p.GameId


					newframe.JoinButton.MouseButton1Click:Connect(function()


						tps:TeleportToPlaceInstance(p.PlaceId, p.GameId, lp)


					end)


				end


			end


		end


	end)


end


