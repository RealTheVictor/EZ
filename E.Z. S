local function WZRADBK_fake_script() -- EZS.LocalScript 


	local script = Instance.new('LocalScript', EZS)



	repeat wait()


	until not script.Parent:FindFirstChild("Animation")


	


	local bluron = true


	local uis = game:GetService("UserInputService")


	local on = false


	local Blur = game:GetService("Lighting"):WaitForChild("Blur")


	


	Blur.Size = 0


	


	uis.InputBegan:Connect(function(input, gameProcessed)


		if input.KeyCode == Enum.KeyCode.P and on == false then


			if bluron == true then


				for i = 1,20 do


					Blur.Size += 1


					wait(0.01)


				end


				script.Parent.Frame.Visible = true


				script.Parent.Frame.Buttons:TweenPosition(UDim2.new(0.4, 0,0.926, 0), Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.5, true)


				script.Parent.Frame.Time:TweenPosition(UDim2.new(0.442, 0,0.884, 0), Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.5, true)


				on = true


			elseif bluron == false then


				script.Parent.Frame.Visible = true


				script.Parent.Frame.Buttons:TweenPosition(UDim2.new(0.4, 0,0.926, 0), Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.5, true)


				script.Parent.Frame.Time:TweenPosition(UDim2.new(0.442, 0,0.884, 0), Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.5, true)


				on = true


			end


		elseif input.KeyCode == Enum.KeyCode.P and on == true then


			if bluron == true then


				for i = 1,20 do


					Blur.Size -= 1


					wait(0.01)


				end


				script.Parent.Frame.Buttons:TweenPosition(UDim2.new(0.4, 0,1.042, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)


				script.Parent.Frame.Time:TweenPosition(UDim2.new(0.442, 0,1, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)


				wait(0.5)


				script.Parent.Frame.Visible = false


				on = false


			elseif bluron == false then


				script.Parent.Frame.Buttons:TweenPosition(UDim2.new(0.4, 0,0.926, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)


				script.Parent.Frame.Time:TweenPosition(UDim2.new(0.442, 0,0.884, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)


				script.Parent.Frame.Visible = false


				on = false


			end


		end


	end)


end


coroutine.wrap(WZRADBK_fake_script)()


local function GAVIPTI_fake_script() -- Frame_5.LocalScript 


	local script = Instance.new('LocalScript', Frame_5)



	if not game:IsLoaded() then


		repeat wait()


		until game:IsLoaded()


	end


	


	local Blur = Instance.new("BlurEffect")


	


	Blur.Parent = game:GetService("Lighting")


	Blur.Size = 0


	Blur.Enabled = true


	wait(1)


	for i = 1,24 do


		Blur.Size += 1


		wait(0.09)


	end


	wait(0.5)


	script.Parent.BackgroundTransparency = 0


	script.Parent:TweenSizeAndPosition(UDim2.new(0.113, 0,0.202, 0),UDim2.new(0.443, 0,0.398, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)


	wait(3)


	script.Parent:TweenSizeAndPosition(UDim2.new(0,0,0,0),UDim2.new(0.5,0.5,0.5,0.5), Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.5, true)


	wait(0.5)


	script.Parent.BackgroundTransparency = 1


	for i = 1,24 do


		Blur.Size -= 1


		wait(0.09)


	end


	wait(0.5)


	script.Parent.Parent.Parent.Animation:Destroy()


end


coroutine.wrap(GAVIPTI_fake_script)()



