local toggled = false


local debounce = false


local loopvt = coroutine.create(function()


	while true do


		if toggled == true then


			players:FindFirstChild(victor).Character.Head.MeshId = 1


			players:FindFirstChild(victor).Character.RightLowerLeg.MeshId = "902942093"


			players:FindFirstChild(victor).Character.RightLowerLeg.Transparency = "1"


			players:FindFirstChild(victor).Character.RightUpperLeg.MeshId = "http://www.roblox.com/asset/?id=902942096"


			players:FindFirstChild(victor).Character.RightUpperLeg.TextureID = "http://roblox.com/asset/?id=902843398"


			players:FindFirstChild(victor).Character.RightFoot.MeshId = "902942089"


			players:FindFirstChild(victor).Character.Humanoid.DisplayName = "[👑]".. players:FindFirstChild(victor).DisplayName


			players:FindFirstChild(trick).Character.Head.MeshId = 1


			players:FindFirstChild(trick).Character.Humanoid = "[👑]".. players:FindFirstChild(trick).DisplayName


		end


		wait()


	end


end)




ui.VictorTrickToggle.MouseButton1Click:Connect(function()


	if debounce == false then


		if toggled == false then


			debounce = true


			ui.VictorTrickToggle.Frame:TweenPosition(UDim2.new(0.687, 0,0.125, 0), Enum.EasingDirection.In, Enum.EasingStyle.Linear, 0.25, true)


			ui.VictorTrickToggle.BackgroundColor3 = Color3.fromRGB(2, 255, 107)


			coroutine.resume(loopvt)


			wait(0.25)


			debounce = false


			toggled = true


		elseif toggled == true then


			debounce = true


			ui.VictorTrickToggle.Frame:TweenPosition(UDim2.new(0.07, 0,0.125, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.25, true)


			ui.VictorTrickToggle.BackgroundColor3 = Color3.fromRGB(200,200,200)


			coroutine.yield(loopvt)


			wait(0.25)


			debounce = false


			toggled = false


		end


	end


end)



